{
  "name": "pytorch-docs-search",
  "version": "0.1.0",
  "description": "PyTorch Documentation Search Tool with MCP integration",
  "entrypoint": {
    "stdio": {
      "command": "python",
      "args": ["-m", "mcp_server_pytorch"]
    },
    "sse": {
      "command": "python",
      "args": ["-m", "mcp_server_pytorch", "--transport", "sse"]
    }
  },
  "requires": [
    "openai",
    "flask",
    "chromadb",
    "tree-sitter",
    "tree-sitter-languages",
    "python-dotenv",
    "tqdm",
    "numpy",
    "flask-cors"
  ]
}